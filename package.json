    "body-parser": "^1.20.2",
    "multer": "^1.4.5-lts.1",
    "better-sqlite3": "^8.6.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "nodemon": "^3.0.1"
  }
}